soma=0
for i in $(seq 1 30)
do 
	res=`java -cp ./build test.LinkedBlockingQueue.Main 4`
	soma=`python -c "print $res+$soma"`
done
python -c "print 'LBQ_4_4000', $soma/30.0"

soma=0
